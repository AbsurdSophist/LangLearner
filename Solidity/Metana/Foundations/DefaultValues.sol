pragma solidity ^0.8.7;

contrat DefaultValues{
    bool public myBool; // default value is false
    uint public myUint; // default value is 0
    address public myAddress; // default value is 0x
    bytes32 public myBytes32; // default value is 0x0
    string public myString; // default value is ""
}